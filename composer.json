{
    "name": "omniphx/forrest",
    "description": "Salesforce / Force.com REST API Client for Laravel 5",
    "license":"MIT",
    "keywords":["salesforce", "laravel", "rest", "force.com", "force"],
    "authors": [
        {
            "name": "Matthew Mitchener",
            "homepage": "http://mattmitchener.com",
            "email": "mattjmitchener@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6",
        "illuminate/cache" : ">5",
        "illuminate/contracts" : ">5",
        "illuminate/config": ">5",
        "illuminate/http" : ">5",
        "illuminate/routing" : ">5",
        "guzzlehttp/guzzle": ">6.0"
    },
    "require-dev": {
        "phpspec/phpspec": "^3.0",
        "henrikbjorn/phpspec-code-coverage": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Omniphx\\Forrest\\": "src/Omniphx/Forrest/"
        }
    },
    "minimum-stability": "stable"
}
